<h1>Listing maps</h1>

<table>
  <tr>
    <th>Cellw</th>
    <th>Cellh</th>
    <th>Dir</th>
    <th>Type</th>
    <th>Subidea</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @maps.each do |map| %>
  <tr>
    <td><%= map.cellw %></td>
    <td><%= map.cellh %></td>
    <td><%= map.dir %></td>
    <td><%= map.ico_type %></td>
    <td><%= map.subidea_id %></td>
    <td><%= link_to 'Show', map %></td>
    <td><%= link_to 'Edit', edit_map_path(map) %></td>
    <td><%= link_to 'Destroy', map, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Map', new_map_path %>
